package com.example.root.rsv.models;

public class ServiceInfo {
    private String name,category,license_plate,previous_service,next_service;

    public ServiceInfo(){
        //Default Constructor.
    }

    public ServiceInfo(String name, String category,String license_plate, String previous_service, String next_service) {
        this.name = name;
        this.category = category;
        this.license_plate = license_plate;
        this.previous_service = previous_service;
        this.next_service = next_service;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getCategory() {
        return category;
    }

    public void setCategory(String category) {
        this.category = category;
    }

    public String getLicense_plate() {
        return license_plate;
    }

    public void setLicense_plate(String license_plate) {
        this.license_plate = license_plate;
    }

    public String getPrevious_service() {
        return previous_service;
    }

    public void setPrevious_service(String previous_service) {
        this.previous_service = previous_service;
    }

    public String getNext_service() {
        return next_service;
    }

    public void setNext_service(String next_service) {
        this.next_service = next_service;
    }
}
